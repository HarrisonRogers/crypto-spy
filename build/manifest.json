{
  "manifest_version": 3,
  "name": "Crypto Spy",
  "version": "1.4.2",
  "description": "Track BTC, ETH & 1000+ cryptocurrency prices live. Free tracker with charts, DEX pairs & Chrome extension alerts.",
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "crypto-spy.png",
    "128": "crypto-spy.png"
  },
  "browser_action": {
    "default_icon": "crypto-spy.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [],
  "host_permissions": ["https://api.cryptospy.app", "https://cryptospy.app"]
}
